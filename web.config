<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="startup.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="DynamicContent">
          <match url=".*" />
          <action type="Rewrite" url="startup.js" />
        </rule>
      </rules>
    </rewrite>
    <iisnode loggingEnabled="false" devErrorsEnabled="true" />
  </system.webServer>
</configuration>
